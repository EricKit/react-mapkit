{
  "name": "react-mapkit",
  "version": "0.1.0",
  "description": "React wrapper for Apple's mapkit.js.",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "pages": "nwb build-demo && gh-pages -d demo/dist",
    "clean": "nwb clean-module && nwb clean-demo",
    "flow": "flow",
    "precommit": "lint-staged",
    "start": "nwb serve-react-demo",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "test": "nwb test-react",
    "keygen": "node keygen"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "colors": "^1.3.0",
    "flow-bin": "0.73.x",
    "gh-pages": "^1.2.0",
    "glamor": "2.20.x",
    "husky": "0.14.x",
    "jsonwebtoken": "8.2.x",
    "lint-staged": "7.1.x",
    "nwb": "0.22.x",
    "prettier": "1.13.x",
    "prompt": "^1.0.0",
    "react": "16.4.x",
    "react-dom": "16.4.x",
    "react-markdown": "3.3.x",
    "typography": "0.16.x",
    "typography-theme-us-web-design-standards": "0.16.x"
  },
  "author": "Chris Drackett <chris@drackett.com>",
  "homepage": "https://github.com/chrisdrackett/react-mapkit",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisdrackett/react-mapkit"
  },
  "bugs": {
    "url": "https://github.com/chrisdrackett/react-mapkit/issues"
  },
  "keywords": [
    "react-component",
    "maps",
    "mapkit",
    "apple"
  ],
  "dependencies": {
    "little-loader": "^0.2.0"
  }
}
