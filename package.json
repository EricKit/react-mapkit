    "@storybook/react": "^3.4.6",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "colors": "^1.3.0",
    "cross-spawn": "^6.0.5",
    "flow-bin": "0.74.x",
    "husky": "0.14.x",
    "jsonwebtoken": "8.2.x",
    "lint-staged": "7.1.x",
